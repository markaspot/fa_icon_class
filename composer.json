{
  "name": "markaspot/fa_icon_class",
  "type": "drupal-module",
  "description": "Font Awesome Icon Field Widget and Formatter",
  "keywords": [
    "Drupal, Font Awesome, Field"
  ],
  "license": "GPL-2.0+",
  "homepage": "https://github.com/markaspot/fa_icon_class",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://github.com/markaspot/fa_icon_class",
    "source": "https://github.com/markaspot/fa_icon_class.git"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "FortAwesome/Font-Awesome",
        "version": "4.6.3",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/FortAwesome/Font-Awesome/archive/v4.6.3.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "itsjavi/fontawesome-iconpicker",
        "version": "1.22",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/itsjavi/fontawesome-iconpicker/archive/1.2.2.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    }
  ]
  ,
  "require": {
    "FortAwesome/Font-Awesome": "4.6.3",
    "itsjavi/fontawesome-iconpicker": "1.22",
  }
}
